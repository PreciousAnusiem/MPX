  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest --detectOpenHandles --forceExit",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "detox test",
    "test:e2e:build": "detox build -c ios.sim.debug",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "clean": "npx react-native clean",
    "reset-cache": "npx react-native start --reset-cache",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "react-native run-ios --configuration Release",
    "bundle:android": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle",
    "bundle:ios": "npx react-native bundle --platform ios --dev false --entry-file index.js --bundle-output ios/main.jsbundle",
    "postinstall": "npx patch-package && cd ios && pod install",
    "flipper": "npx react-native run-ios --simulator=\"iPhone 14 Pro\"",
    "adb": "adb reverse tcp:8081 tcp:8081",
    "prepare": "husky install",
    "prebuild": "npm run lint && npm run type-check && npm run test",
    "analyze": "npx @react-native-community/cli doctor",
    "security-check": "npm audit --audit-level moderate",
    "deps-check": "npx ncu -i",
    "generate:icons": "npx @bam.tech/react-native-make generate-icons",
    "generate:splash": "npx @bam.tech/react-native-make generate-splashscreen",
    "codegen": "npx react-native codegen",
    "metro": "npx react-native start --experimental-debugger"
  },
